{
  "name": "karet.xhr",
  "version": "0.2.0",
  "description": "An observable wrapper for XMLHttpRequest using Kefir",
  "module": "dist/karet.xhr.es.js",
  "main": "dist/karet.xhr.cjs.js",
  "sideEffects": false,
  "scripts": {
    "dist": "rollup -c",
    "lint": "eslint src test",
    "prepare": "npm run lint && npm run dist && npm run test",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "test": "npm run test:bundle && node test/server.js && nyc report && nyc report -r html mocha",
    "test:bundle": "nyc instrument dist/karet.xhr.es.js test/generated && rollup -c test/rollup.config.js",
    "watch": "concurrently \"npm run watch:dist\" \"npm run watch:test\"",
    "watch:dist": "watch-run -d 50 -i -p 'src/**/*.js' npm run dist",
    "watch:test": "watch-run -d 50    -p 'dist/karet.xhr.es.js,test/*.js' npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/karet.xhr.git"
  },
  "keywords": [
    "direct-style",
    "kefir",
    "observable",
    "property",
    "reactive"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/karet.xhr/issues"
  },
  "homepage": "https://github.com/calmm-js/karet.xhr#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-annotate-pure-calls": "^0.3.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "codecov": "^3.0.3",
    "concurrently": "^3.6.0",
    "eslint": "^5.0.1",
    "express": "^4.16.3",
    "kefir": "^3.8.3",
    "mocha": "^5.2.0",
    "mocha-headless-chrome": "^2.0.1",
    "nyc": "^12.0.2",
    "ramda": "^0.25.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "watch-run": "^1.2.5"
  },
  "dependencies": {
    "infestines": "^0.4.9",
    "karet.lift": "^1.1.0",
    "kefir.partial.lenses": "^13.14.1",
    "partial.lenses.validation": "^1.0.4"
  },
  "peerDependencies": {
    "kefir": "^3.7.0"
  }
}
